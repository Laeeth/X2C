#include <QtCore/QtCore>
#include "Root.h"

int main(int argc, char* argv[]) {
	QFile file("/home/adam/programming/workspace/X2C/test.xml");
	Root *r = Root::fromFile(file);
	qDebug() << "read";
	QList<Integer*> is = r->getIntegerList();
	for (int i = 0; i < is.size(); i++) {
		qDebug() << is.at(i)->getValue();
	}
	QList<UunsignedShort*> us = r->getUunsignedShortList();
	for (int i = 0; i < us.size(); i++) {
		qDebug() << us.at(i)->getValue();
	}
	QList<Bbyte*> bs = r->getBbyteList();
	for (int i = 0; i < bs.size(); i++) {
		qDebug() << bs.at(i)->getValue();
	}
	QList<Llong*> ls = r->getLlongList();
	for (int i = 0; i < ls.size(); i++) {
		qDebug() << ls.at(i)->getValue();
	}
	QList<Llist*> lls = r->getLlistList();
	for (int i = 0; i < lls.size(); i++) {
		QList<long long> ll = lls.at(i)->getValue();
		for(int j = 0; j < ll.size(); j++) {
			qDebug() << ll.at(j);
		}
	}

	return EXIT_SUCCESS;
}
